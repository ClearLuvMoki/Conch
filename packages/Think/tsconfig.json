{
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "checkJs": false,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "strict": false,
    "noUnusedLocals": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/Constant/*": [
        "./src/Constant/*"
      ],
      "@/src/*": [
        "./src/*"
      ],
      "@/types/*": [
        "../../src/types/*"
      ],
      "@/Components/*": [
        "./src/Components/*"
      ],
      "@/Layout/*": [
        "./src/Layout/*"
      ],
      "@/Pages/*": [
        "./src/Pages/*"
      ],
      "@/Router/*": [
        "./src/Router/*"
      ],
      "@/RIpc/*": [
        "../../src/render/RIpc/*"
      ],
      "@/Utils/*": [
        "./src/Utils/*"
      ],
      "@/Stores/*": [
        "./src/Stores/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
