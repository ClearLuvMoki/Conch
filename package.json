  },
  "devDependencies": {
    "@antv/g2": "^5.1.6",
    "@nextui-org/react": "^2.1.13",
    "@rspack/cli": "latest",
    "@types/deep-equal": "^1.0.3",
    "@types/node": "^20.8.7",
    "@types/node-os-utils": "^1.3.3",
    "@types/os-utils": "^0.0.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/sortablejs": "^1.15.4",
    "autoprefixer": "^10.4.16",
    "await-to-js": "^3.0.0",
    "dayjs": "^1.11.10",
    "deep-equal": "^2.2.2",
    "electron": "^27.0.1",
    "electron-builder": "^24.6.4",
    "electron-log": "^4.4.8",
    "electronmon": "^2.0.2",
    "framer-motion": "^10.16.4",
    "node-os-utils": "^1.3.7",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-nesting": "^12.0.1",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.17.0",
    "sortablejs": "^1.15.0",
